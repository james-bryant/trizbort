{{{className object.name}}}:
  kind: {{{kindToStr object.kind}}}
  {{#if object.description}}
  description: > 
    {{> DescriptionPartial content=object.description }}
  {{/if}}
  symbol: {{object.symbol}}
  {{#if object.data}}
  data: >
    {{> DescriptionPartial content=object.data }}
  {{/if}}
  {{#if object.color}}
  color: {{object.color}}
  {{/if}}
  {{#if object.content.length}}
  objects:
  {{#each object.content}}
    {{> yamlObject object=this container=../object }}
  {{/each}}
  {{/if}}
